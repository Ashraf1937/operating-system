#include <stdio.h>
#include <unistd.h>

int main() {
    pid_t pid, ppid;

    pid = fork();
    if (pid == 0) {
        // Child process
        printf("Child Process: PID = %d, PPID = %d\n", getpid(), getppid());
    } else {
        // Parent process
        printf("Parent Process: PID = %d, PPID = %d\n", getpid(), getppid());
    }

    return 0;
}
